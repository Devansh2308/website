import { createFragmentContainer, graphql } from "react-relay";

function Leaderboard{
    return null;

}





export const fragment= createFragmentContainer(Leaderboard, {
    leaderboard: graphql`
        fragment Leaderboard_leaderboard on leaderboard @argumentDefinitions(
  first: {type: "Int", defaultValue: 10}
)  {
            leaderBoard( first: $first) {
                                        edges {
                                            node {
                                                id
                                                userName
                                                name
                                                email
                                                phone
                                                college
                                                year
                                                country
                                                admin
                                                totalQuestionsAnswered
                                                rank {
                                                    id
                                                    rank
                                                }
                                                cursor
                                            }
                                            pageInfo {
                                                endCursor
                                                hasNextPage
                                            }
                                        }
    `
});
